{
    "name": "@bootstrap-brand/source",
    "version": "0.0.1",
    "description": "Bootstrap your brand's app.",
    "license": "UNLICENSE",
    "author": {
        "name": "Default One",
        "email": "setdefaultone@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/SetDefaultOne/bootstrap-brand.git"
    },
    "scripts": {
        "develop": "turbo run develop",
        "format": "turbo run format:source format",
        "format:source": "prettier . --write --cache",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "build": "turbo run build",
        "pass": "turbo run format:source format lint test build",
        "prepare": "husky"
    },
    "devDependencies": {
        "prettier": "3.3.3",
        "turbo": "2.0.6"
    },
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "engines": {
        "node": "20.15.1"
    },
    "packageManager": "npm@10.7.0"
}
