{
    "name": "@bootstrap-brand/backend",
    "version": "0.0.1",
    "scripts": {
        "develop": "nest start --watch",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,test}/**/*.ts\"",
        "test": "turbo run test:unit test:cov test:e2e",
        "build": "nest build",
        "start": "nest start",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "test:unit": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage --passWithNoTests",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json --passWithNoTests",
        "typeorm:migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d ./src/database/migrations/datasource.options.ts",
        "typeorm:migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/database/migrations/datasource.options.ts"
    },
    "dependencies": {
        "@bootstrap-brand/sdk": "*",
        "@nestjs/common": "10.3.10",
        "@nestjs/config": "3.2.3",
        "@nestjs/core": "10.3.10",
        "@nestjs/mongoose": "^10.0.10",
        "@nestjs/platform-express": "10.3.10",
        "@nestjs/typeorm": "^10.0.2",
        "bcrypt": "^5.1.1",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "mongoose": "^8.5.1",
        "pg": "^8.12.0",
        "reflect-metadata": "0.2.2",
        "rxjs": "7.8.1",
        "typeorm": "^0.3.20"
    },
    "devDependencies": {
        "@nestjs/cli": "10.4.2",
        "@nestjs/schematics": "10.1.2",
        "@nestjs/testing": "10.3.10",
        "@types/bcrypt": "^5.0.2",
        "@types/express": "4.17.21",
        "@types/jest": "29.5.12",
        "@types/node": "20.14.10",
        "@types/supertest": "6.0.2",
        "@typescript-eslint/eslint-plugin": "7.16.0",
        "@typescript-eslint/parser": "7.16.0",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "jest": "29.7.0",
        "prettier": "3.3.3",
        "source-map-support": "0.5.21",
        "supertest": "7.0.0",
        "ts-jest": "29.2.2",
        "ts-loader": "9.5.1",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.5.3"
    }
}
